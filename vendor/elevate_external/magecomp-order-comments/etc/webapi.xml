<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/guest-comment/:cartId/set-transactional-comment" method="POST">
        <service class="Magecomp\Ordercomment\Api\GuestOrdercommentManagementInterface" method="saveOrdercomment"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/comment/mine/set-transactional-comment" method="POST">
        <service class="Magecomp\Ordercomment\Api\OrdercommentManagementInterface" method="saveOrdercomment"/>
        <resources>
            <resource ref="self" />
        </resources>
        <data>
            <parameter name="cartId" force="true">%cart_id%</parameter>
        </data>
    </route>
</routes>