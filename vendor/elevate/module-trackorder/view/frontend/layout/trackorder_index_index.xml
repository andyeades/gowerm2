<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceContainer name="content">
        <block class="Elevate\TrackOrder\Block\Index" name="elevate_trackorder_index_index" template="Elevate_TrackOrder::trackorder.phtml" />
    </referenceContainer>
    <referenceBlock name="page.main.title">
    <action method="setPageTitle">
        <argument translate="true" name="title" xsi:type="string">Track Your Order</argument>
    </action>
</referenceBlock>
</page>