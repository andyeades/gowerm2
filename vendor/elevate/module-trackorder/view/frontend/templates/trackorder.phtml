<script type="text/javascript">

    jQuery('#trackordersubmit').on('click', function(){

        var increment_id = jQuery('#trackorder_increment_id').val();
        var email_address = jQuery('#trackorder_email_address').val();

        var url = '/trackorder/index/getorder';

        // url = url.replace("https://","http://"); // New Code

       var data = {
            'increment_id': increment_id,
                'email_address': email_address // <-- the $ sign in the parameter name seems unusual, I would avoid it
        };
        // jQuery('#ajax_loader').show();

        jQuery.ajax({
            url: url,
            dataType: 'json',
            type: 'post',
            data: data,
            success: function (data) {


                // jQuery('#inject_me').html(data);



                if(data.html){

                    jQuery('#order_data').html(data.html);


                }
                else{

                    alert("Order not found");
                }

            }
        });

    });




</script>
