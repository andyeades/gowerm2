<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">elevate_cartassignments_cartassignments_listing.elevate_cartassignments_cartassignments_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>elevate_cartassignments_cartassignments_columns</spinner>
		<deps>
			<dep>elevate_cartassignments_cartassignments_listing.elevate_cartassignments_cartassignments_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new CartAssignments</label>
			</button>
		</buttons>
	</settings>
	<dataSource component="Magento_Ui/js/grid/provider" name="elevate_cartassignments_cartassignments_listing_data_source">
		<settings>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Elevate_CartAssignments::CartAssignments</aclResource>
		<dataProvider class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider" name="elevate_cartassignments_cartassignments_listing_data_source">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>cartassignments_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="elevate_cartassignments_cartassignments_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">elevate_cartassignments_cartassignments_listing.elevate_cartassignments_cartassignments_listing.elevate_cartassignments_cartassignments_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">cartassignments_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" path="elevate_cartassignments/CartAssignments/inlineEdit" xsi:type="url"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">elevate_cartassignments_cartassignments_listing.elevate_cartassignments_cartassignments_listing.elevate_cartassignments_cartassignments_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>cartassignments_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="cartassignments_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="enable_addon">
			<settings>
				<filter>text</filter>
				<label translate="true">enable_addon</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
	
		<column name="title">
			<settings>
				<filter>text</filter>
				<label translate="true">title</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="addon_type">
			<settings>
				<filter>text</filter>
				<label translate="true">addon_type</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
	<actionsColumn class="Elevate\CartAssignments\Ui\Component\Listing\Column\CartAssignmentsActions" name="actions">
			<settings>
				<indexField>cartassignments_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>		
	</columns>
</listing>
