<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="sales_quote_item_set_product">
        <observer name="set_item_delivery_attributes" instance="Elevate\Delivery\Observer\SetItemDeliveryAttributes" />
    </event>
    <event name="sales_model_service_quote_submit_before">
        <observer name="set_custom_delivery_attributes" instance="Elevate\Delivery\Observer\SetCustomDeliveryAttributes"/>
    </event>

    <event name="checkout_submit_all_after">
        <observer name="set_custom_attributes_after" instance="Elevate\Delivery\Observer\SetCustomCheckoutAfterAttributes"/>
    </event>

    <event name="checkout_type_multishipping_create_orders_single">
        <observer name="set_multiship_attributes" instance="Elevate\Delivery\Observer\SetCustomMultishipAttributes"/>
    </event>
</config>
