<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/elevate-themeoptions/options">
		<service class="Elevate\Themeoptions\Api\OptionsRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Elevate_Themeoptions::Options_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/elevate-themeoptions/options/search">
		<service class="Elevate\Themeoptions\Api\OptionsRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Elevate_Themeoptions::Options_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/elevate-themeoptions/options/:entityId">
		<service class="Elevate\Themeoptions\Api\OptionsRepositoryInterface" method="getById"/>
		<resources>
			<resource ref="Elevate_Themeoptions::Options_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/elevate-themeoptions/options/:entityId">
		<service class="Elevate\Themeoptions\Api\OptionsRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Elevate_Themeoptions::Options_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/elevate-themeoptions/options/:entityId">
		<service class="Elevate\Themeoptions\Api\OptionsRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Elevate_Themeoptions::Options_delete"/>
		</resources>
	</route>
</routes>
