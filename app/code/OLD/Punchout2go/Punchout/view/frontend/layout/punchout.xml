    </head>
    <body>
        <!--<attribute name="class" value="is-punchout-session" />-->

            <referenceBlock name="header.links">
                <block class="Punchout2go\Punchout\Block\Session\Close" name="punchout-close"
                    template="punchout_close.phtml" after="-">
                    <arguments>
                        <argument name="class" xsi:type="string">punchout-close</argument>
                    </arguments>
                </block>
            </referenceBlock>


            <referenceContainer name="footer">
            <block class="Punchout2go\Punchout\Block\Session" name="punchout.session.js" template="Punchout2go_Punchout::js/punchout_config.phtml" cacheable="true">
            </block>
        </referenceContainer>
        <!--
        <referenceBlock name="head.components">
